#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"enter number of elements to add :";
    cin>>n;
    int x[n];
    for(int i=0;i<n;i++){
        cout<<"enter "<<i+1<<"th number :";
        cin>>x[i];
    }
    int sum=0;
    for(int i=0;i<n;i++){
        sum+=x[i];
    }
    cout<<"sum of the elements is :"<<sum;
    //return 0;
}
